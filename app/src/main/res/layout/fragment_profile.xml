        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_genderpro_l" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
