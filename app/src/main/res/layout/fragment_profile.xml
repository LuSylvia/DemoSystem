        android:padding="5dp"
        android:paddingEnd="10dp"
        android:gravity="right"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_emailpro_l"
        app:layout_constraintTop_toTopOf="@+id/tv_emailpro_l" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey81"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_emailpro_l"></View>

    <TextView
        android:id="@+id/tv_idcardpro_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
